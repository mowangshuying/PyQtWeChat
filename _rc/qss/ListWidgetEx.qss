QListWidget {
    background-color: rgb(243, 243, 243);
    outline: none;
    border: 1px solid transparent;
    border-radius: 0px;
    padding: 4px;
}

QListWidget::item {
    background-color: transparent;
}

QListWidget::item:hover {
    /* background-color: rgb(234, 234, 234); */
    background-color: transparent;
}

QListWidget::item:selected {
    /* background-color: rgb(234, 234, 234); */
    background-color: transparent;
}